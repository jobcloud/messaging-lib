{
  "name": "jobcloud/messaging-lib",
  "description": "Jobcloud kafka lib",
  "keywords": [
    "kafka"
  ],
  "license": "proprietary",
  "repositories": [
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:jobcloud/unity-coding-standards.git"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-rdkafka": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1.4",
    "squizlabs/php_codesniffer": "^3.2.3",
    "php-mock/php-mock-phpunit": "^2.1.1",
    "jobcloud/unity-coding-standards": "dev-master",
    "kwn/php-rdkafka-stubs": "^1.0.2"
  },
  "autoload": {
    "psr-4": {
      "Jobcloud\\Messaging\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0-dev"
    }
  }
}
