{
  "name": "jobcloud/messaging-lib",
  "description": "Jobcloud php messaging library (kafka)",
  "keywords": [
    "php",
    "kafka",
    "messaging"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-pcntl": "^7.1",
    "ext-rdkafka": "^3.0.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2.3|^7.5.13",
    "squizlabs/php_codesniffer": "^3.4.2",
    "phpstan/phpstan": "^0.11.8",
    "php-mock/php-mock-phpunit": "^2.4",
    "kwn/php-rdkafka-stubs": "^1.3.0",
    "phpstan/extension-installer": "^1.0.1",
    "mavimo/phpstan-junit": "^0.2.1"
  },
  "autoload": {
    "psr-4": {
      "Jobcloud\\Messaging\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
