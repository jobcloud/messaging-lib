  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-rdkafka": "^3.0.5|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.16|^8.4",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.11.16",
    "php-mock/php-mock-phpunit": "^2.5",
    "kwn/php-rdkafka-stubs": "^1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Jobcloud\\Messaging\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
