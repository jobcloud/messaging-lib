{
  "name": "jobcloud/kafka-lib",
  "description": "Jobcloud kafka lib",
  "keywords": [
    "kafka"
  ],
  "license": "proprietary",
  "repositories": [
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:jobcloud/unity-coding-standards.git"
    }
  ],
  "require": {
    "php": "~7.1",
    "ext-rdkafka": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4.3",
    "sebastian/phpcpd": "^3.0",
    "pdepend/pdepend": "^2.5",
    "phploc/phploc": "^4.0",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.1.1",
    "php-mock/php-mock-phpunit": "~2.0",
    "jobcloud/unity-coding-standards": "dev-master",
    "kwn/php-rdkafka-stubs": "^1.0.2"
  },
  "autoload": {
    "psr-4": {
      "Jobcloud\\Kafka\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0-dev"
    }
  }
}